<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.video.f4mTester" version="3.0.0" name="F4mTester" provider-name="Shani">
    <requires>
        <import addon="xbmc.python" version="2.1.0"/>
        <import addon="script.video.F4mProxy" version="2.7.1"/>
    </requires>
    <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>video</provides>
    </extension>


    <extension point="xbmc.addon.metadata">
        <summary>f4mTester</summary>
        <description></description>
        <platform>all</platform>
    </extension>
</addon>

  <addon id="plugin.video.kodivertido-X"
       name="KODIvertiDO X"
       version="1.1.0"
       provider-name="Grupo Kodidivertido">
  <requires>
	  <import addon="script.module.beautifulsoup4" />
    <import addon="script.module.requests" />
    <import addon="plugin.video.youtube" optional="true" />
    <import addon="script.module.resolveurl" />
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource"
            library="default.py">
        <provides>video</provides>
  </extension>

  <extension point="xbmc.addon.metadata">
    <summary>kodivertido</summary>
    <description>kodivertido</description>
	  <disclaimer>kodivertido</disclaimer>
    <platform>all</platform>
	<assets>
    <icon>icon.gif</icon>
    <fanart>fanart.jpg</fanart>
    </assets>
  </extension>
</addon>

    <addon id="repository.kodivertidoX" name="Repositorio KODIvertiDO" version="1.1.0" provider-name="Mendelux">
        <requires>
            <import addon="xbmc.addon" version="12.0.0"/>
        </requires>
        <extension point="xbmc.addon.repository" name="Repositorio KODIvertiDO">
            <info compressed="false">https://raw.githubusercontent.com/mendelux/repositorioKodivertido/master/_repo/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/mendelux/repositorioKodivertido/master/_repo/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/mendelux/repositorioKodivertido/master/_repo/</datadir>
            <hashes>false</hashes>
        </extension>
        <extension point="xbmc.addon.metadata">
            <summary>Grupo KODIvertiDO</summary>
            <description>Actualiza tu Addon KODIvertiDO X</description>
            <platform>all</platform>
    </extension>
</addon>
</addons>
